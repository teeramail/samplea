  "type": "module",
  "scripts": {
    "build": "next build",
    "build:with-db": "npx drizzle-kit push && next build",
    "check": "next lint && tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "tsx src/server/db/seed-script.ts",
    "db:seed-products": "tsx src/scripts/seed-products.ts",
    "db:migrate-payment-fields": "tsx drizzle/payment_fields_migration.ts",
    "db:validate": "tsx src/scripts/db-schema-validator.ts",
    "db:update-schema-json": "tsx src/scripts/update-schema-json.ts",
    "db:analyze": "tsx src/scripts/db-analyzer.ts",
    "db:check": "tsx src/scripts/db-schema-manager.ts validate",
    "db:push-schema": "tsx src/scripts/db-schema-manager.ts push",
    "db:pull-schema": "tsx src/scripts/db-schema-manager.ts pull",
    "db:document": "tsx src/scripts/db-schema-manager.ts document",
    "db:manage": "tsx src/scripts/db-schema-manager.ts all",
    "db:fix-production": "npx drizzle-kit push",
    "db:fix-category": "tsx src/scripts/fix-category-table.ts",
    "db:introspect": "npx drizzle-kit introspect",
    "db:sync-check": "node scripts/compare_postgres_drizzle.js",
    "db:backup-schema": "cp src/server/db/schema.ts src/server/db/schema.ts.backup",
    "db:sync": "node scripts/schema_sync_helper.js",
    "db:compare-json": "node scripts/final_corrected_compare.js",
    "timezone:analyze": "tsx src/scripts/fixTimezones.ts",
    "dev": "next dev --turbo -H 0.0.0.0",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit"
  },
